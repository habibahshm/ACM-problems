
public class Os {
	static int mod(int x, int y) {
		if (x < y)
			return x;
		else {

			while (x >= y) {
				x = x - y;
			}

			return y;
		}
	}

	static int div(int x, int y) {
		int i = 0;
		if (x < y)
			return 0;
		else {

			while ((i + 1) * y <= x)
				i = i + 1;
			return i;
		}
	}

	public static void main(String[] args) {
		System.out.println(div(4, 2) + " " + mod(3, 5));
	}
}
